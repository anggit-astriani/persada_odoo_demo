<odoo>
    <record id="view_inventory_return_product_wizard" model="ir.ui.view">
        <field name="name">inventory.return.product.wizard.form</field>
        <field name="model">inventory.return.product.wizard</field>
        <field name="arch" type="xml">
            <form string="Return Product">
                <group>
                    <field name="delivery_id" readonly="1"/>
                </group>
                <field name="line_ids">
                    <tree editable="bottom">
                        <field name="select"/>
                        <field name="product_id"/>
                        <field name="code_product"/>
                    </tree>
                </field>
                <footer>
                    <button name="action_confirm" string="Returnnnn" type="object" class="btn-primary"/>
                    <button string="Cancel" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

    <act_window
        id="action_inventory_return_product_wizard"
        name="Return Product"
        res_model="inventory.return.product.wizard"
        view_mode="form"
        target="new"/>
</odoo>
